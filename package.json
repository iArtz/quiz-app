{
  "name": "quiz-app",
  "description": "Quiz App Demo",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.4.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "code-lint": "eslint . --ext '.js' --cache"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": ["react-app", "plugin:react/recommendede", "airbnb"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffur": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeeatures": {
        "js": true
      },
      "ecmaVersion":  2018,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/jsx-filename-extension": [
        "warn", {
          "extension": [".js", ".jsx"]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error", {
          "devDependencies": true
        }
      ]
    },
    "ignorePatterns": [
      "build",
      "node_modules",
      "node_modules.nosync"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "acorn-jsx": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "tsutils": "^3.17.1"
  }
}
